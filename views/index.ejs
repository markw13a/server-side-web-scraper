<!DOCTYPE html>
<html>
  <head>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
	<table>
		<tr>
			<th>Job Title</th>
			<th>Company</th>
			<th>Location</th>
			<th>Extracted from</th>
		</tr>
		<% for(let i =0; i<data.length; i++){%>
			<tr>
				<td><%=data[i].jobTitle%></td>
				<td><%=data[i].companyName%></td>
				<td><%=data[i].jobLocation%></td>
				<td><%=data[i].website%></td>
			</tr>
		<%}%>
	</table>
  </body>
</html>
